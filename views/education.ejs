<%- include('layout/header.ejs') -%>

<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
  </head>
  <body>


    <div class="px-4 py-1  text-center">
        <h1 class="fw-normal mt-5">My Eduction</h1>
        <div class="col-lg-6 mx-auto">
          <p class="lead mb-4" style="color: rgb(95, 94, 94);">“Education is one thing no one can take away from you.”</p>
        </div>

        <div class="  text-center">
           <img src="./images/eduction.png" width="100px" alt="">
            </div>


        <div class="container px-2 py-2" id="hanging-icons">
            <div class="row g-4 py-5 row-cols-1 row-cols-lg-4">
              <div class="col d-flex align-items-start">
                <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
                </div>
                <div>
                  <h3 class="fs-2 text-body-emphasis">Secondary School Certificate</h3>
                  <p style="color: rgb(87, 86, 86);">I have Completed my Secondary Schooling From <span><b>St. joseph's Academy</b></span>  , Which belongs to <span><b>Council for Indian School Certificate Examinations [CICSE] board</b></span> .</p>
                  
                </div>
              </div>
              <div class="col d-flex align-items-start">
                <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
                </div>
                <div>
                  <h3 class="fs-2 text-body-emphasis">Intermediate Education</h3>
                  <p style="color: rgb(95, 94, 94);">I have Completed my Post-Secondary Schooling From <span><b>St. joseph's Academy</b></span>  , Which belongs to <span><b>Council for Indian School Certificate Examinations [CICSE] board</b></span> .</p>
                  
                </div>
              </div>
              <div class="col d-flex align-items-start">
                <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
                </div>
                <div>
                  <h3 class="fs-2 text-body-emphasis">Undergraduate Degree</h3>
                  <p style="color: rgb(95, 94, 94);">I have Completed my Undergraduate Degree From <span><b> Vidyavati Mukand Lal Girls College Ghaziabad</b></span>, Which belongs to <span><b>Chaudhary Charan Singh University, Meerut[CCSU]</b></span> .</p>
                  
                </div>
              </div>
              <div class="col d-flex align-items-start">
                <div class="icon-square text-body-emphasis bg-body-secondary d-inline-flex align-items-center justify-content-center fs-4 flex-shrink-0 me-3">
                </div>
                <div>
                  <h3 class="fs-2 text-body-emphasis">Postgraduate Degree</h3>
                  <p style="color: rgb(95, 94, 94);">Now, I am Pursuing Postgraduate Degree From <span><b>R.D.Engineering College Duhai Ghaziabad</b></span>  , Which belongs to <span><b>Abdul Kalam Technical University (AKTU)</b></span> .</p>
                  
                </div>
              </div>

            </div>
          </div>



    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
  </body>
</html>